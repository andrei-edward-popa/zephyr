config COUNTER_RTC_RPI_PICO
	bool "Raspberry Pi Pico RTC Driver"
	default y
	depends on DT_HAS_RASPBERRYPI_PICO_RTC_ENABLED
	select PICOSDK_USE_RTC
	depends on RESET
	help
	  Enable support for Raspberry Pi Pico RTC driver
